<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>INICIO</title>
</head>
<body>
    <h1>Listas de ususarios</h1>
    <table>
        <tr>
            <th>ID</th>
            <th>Nombre</th>
            <th>Email</th>
            <th>Acciones</th>
        </tr>
        <% users.forEach(user =>{%)>
            <tr>
                <td><%= user.id %></td>
                <td><%= user.name%></td>
                <td><%= user.email%></td>
                <td>
                    <a href="/edit/<%user.id%>">Editar</a>
                    <a href="/delete/<%user.id%>">Eliminar</a>
                </td>
            </tr>
        <% }); %>
    </table>
</body>
</html>